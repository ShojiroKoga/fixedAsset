<!DOCTYPE html>
<html>
  <head>
    <title>FixedAssetsManage</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <header>
    <h1>固定資産台帳</h1>


  </header>


  <body>
    <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "#{key}") %>
    <% end %>

    <% if logged_in? %>
      <%= link_to "ログアウト", session_path(current_user.id), method: :delete, class: 'btn btn-default' %>
    <% else %>
      <%= link_to "ログイン", new_session_path , class: 'btn btn-default'%>
    <% end %>

    <%= yield %>
    

  </body>
</html>
